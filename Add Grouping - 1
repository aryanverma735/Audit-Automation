SELECT * FROM
GRPG G JOIN 
CD_VAL C ON G.GRPG_TYPE_CD = C.CD_VAL_CD
JOIN CD_VAL R ON G.GRPG_ROLE_CD = R.CD_VAL_CD
JOIN GRPG_ALT_IDFCTN A ON G.GRPG_ID = A.GRPG_ID
JOIN CD_VAL CC ON A.ALTID_SOR_CD = CC.CD_VAL_CD
JOIN CD_VAL CD ON A.ALTID_TYPE_CD = CD.CD_VAL_CD
JOIN GRPG_PROV_ORG_RLTNSHP R ON R.GRPG_ID = G.GRPG_ID

WHERE G.GRPG_NM = 'Community Health Centers #11'
AND C.CD_VAL_NM = 'PCP Capitation-Individual PCP'
AND R.CD_VAL_NM = 'Main'
AND G.PROV_DATA_OWNR_CD = 'ANTHEM'
AND G.GRPG_EFCTV_DT = TO_DATE('01/01/1901', 'MM/DD/YYYY')
AND CC.CD_VAL_NM IN 'GBD'
AND CD.CD_VAL_NM IN 'GBD Facets IPA PRPR ID'
AND A.ALTID_TXT IN '24906396'
