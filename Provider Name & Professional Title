SELECT * 
FROM PROV P
JOIN IP_PRFSNL_TTL_SFX T
ON P.MSTR_PROV_ID = T.MSTR_PROV_ID
JOIN CD_VAL C
ON T.PRFSNL_TTL_SFX_CD = C.CD_VAL_CD
WHERE 
P.MSTR_PROV_ID = '1101584769'
AND P.PROV_FRST_NM = 'Valicia'
AND UPPER(P.PROV_MID_NM) = UPPER('L')
AND UPPER(P.PROV_LAST_NM) = UPPER('Bradford')
AND UPPER(C.CD_VAL_NM) = UPPER('RSW')
AND UPPER(C.SHRT_DESC_TXT) = UPPER('RSW')
