WITH CTE AS
(SELECT *
FROM PROV P
JOIN PROV_SPCLTY PS
  ON P.MSTR_PROV_ID = PS.MSTR_PROV_ID
JOIN BOT_PRCG_SPCLTY_XREF PN 
  ON PS.SPCLTY_CD = PN.SPCLTY_CD

WHERE P.MSTR_PROV_ID = '1105207359'
  AND PN.SPCLTY_NM = 'Ambulatory Surgery Center'
-- CACTUS-"8010"
  AND PS.PROV_SPCLTY_SOR_CD = '8010')

SELECT 
CASE WHEN COUNT(*) > 0 THEN 'Audit Pass' ELSE 'Audit Fail' END AS Audit_result 
FROM CTE
