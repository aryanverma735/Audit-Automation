WITH CTE AS (
SELECT * 
FROM GRPG_PROV_ORG_RLTNSHP GP
WHERE GP.GRPG_ID = '4000076241'
AND GP.MSTR_PROV_ID = '1101537606'
AND GP.GRPG_RLTNSHP_TYPE_CD = ''
AND GP.GRPG_PRVORG_RLTN_EFCTV_DT = TO_DATE('', 'MM/DD/YYYY')
)

SELECT CASE WHEN COUNT(*) > 0 THEN 'Audit Pass' ELSE 'Audit Fail' END AS "Audit Result"
FROM CTE
