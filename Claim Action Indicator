
SELECT
  CASE
    WHEN COUNT(*) > 0 THEN 'Audit Pass'
    ELSE 'Audit Fail'
  END AS audit_status
FROM PROV_CLMACT
WHERE MSTR_PROV_ID = ''
AND CLMACT_SRC_CD = ''
AND CLMACT_TYPE_CD = ''
AND ST_PRVNC_CD = ''
AND CLMACT_EFCTV_DT = TO_DATE('', 'MM/DD/YYYY')
AND CLMACT_CRTRIA_TYPE_CD = ''
AND DIR_HOLD_IND = ''
